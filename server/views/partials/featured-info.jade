.panel.panel-primary
    .panel-heading.text-center Featured Info
    .panel-body
        .row(ng-repeat="item in main.info | orderBy:'id' | filter: updated")
            .col-md-6
                h5 {{item.name}}
            .col-md-6
                p {{item.content}}

